public class Main {
    public static void main(String[] args) {
        for (int y = 0; y!=3001; y++) {
            if (y==0) {
                System.out.println("don't exist");}
            if (y<100&&y%4==0) {
                System.out.println(y);}
            if (y>=100&&y<400&&y%4==0&&y%100!=0) {
                System.out.println(y);}
            if ((y>=400&&y%4==0&&y%100!=0)||(y>=400&&y%400==0)) {
                System.out.println(y);}
        }
    }}