public class Main {
    public static void main(String[] args) {
for (int i=1; i!=11; i++) {
    System.out.println(i);}
for (int i=10; i!=0; i--)
{
    System.out.println(i);}
for (int c = 0; c<18; c=c+2) {
    System.out.println(c);}
for (int u = 10; u!=-11; u--) {System.out.println(u);}
    }
    }